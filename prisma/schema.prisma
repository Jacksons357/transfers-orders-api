generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transfer {
  id          String   @id @default(cuid())
  product     String?
  code        String
  quantity    Int
  lote        String?
  validate    String?
  destination String
  createdAt   DateTime @default(now())
  updadteAt   DateTime @default(now()) @updatedAt

  @@map("transfers")
}
